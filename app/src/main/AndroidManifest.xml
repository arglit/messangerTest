<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.argalit.dev.TestApp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name="com.argalit.dev.TestApp.TestApp_Offline">
        <activity android:name="com.argalit.dev.TestApp.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.argalit.dev.TestApp.MainActivity" />
        <activity android:name="com.argalit.dev.TestApp.StartPageActivity" />
        <activity
            android:name="com.argalit.dev.TestApp.LoginActivity"
            android:parentActivityName="com.argalit.dev.TestApp.StartPageActivity" />
        <activity
            android:name="com.argalit.dev.TestApp.RegisterActivity"
            android:parentActivityName="com.argalit.dev.TestApp.StartPageActivity"></activity>
        <activity android:name="com.argalit.dev.TestApp.SettingsActivity"></activity>
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name="com.argalit.dev.TestApp.StatusActivity"
            android:parentActivityName="com.argalit.dev.TestApp.SettingsActivity"></activity>
        <activity
            android:name="com.argalit.dev.TestApp.AllUsersActivity"
            android:parentActivityName="com.argalit.dev.TestApp.MainActivity" />
        <activity android:name="com.argalit.dev.TestApp.ProfileActivity"></activity>
    </application>

</manifest>